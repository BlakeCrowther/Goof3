[ VariableDeclaration {
    access: 'global',
    type: 'object',
    id: Identifier { name: 'myObj' },
    initializers:
     ObjectExp {
       Properties:
        [ Field {
            type: 'whole_number',
            key: Identifier { name: 'x' },
            value: Literal { type: 'number', value: 15 } },
          Field {
            type: 'array_of_chars',
            key: Identifier { name: 'name' },
            value: Literal { type: 'string', value: 'Doofus' } },
          Method {
            parameters: null,
            body:
             [ CallExpression {
                 callee: 'poof',
                 args: Literal { type: 'string', value: 'hi there' } } ],
            key: Identifier { name: 'sayHello' } } ] } },
  MemberExpression {
    object: Identifier { name: 'myObj' },
    property: Identifier { name: 'x' } } ]