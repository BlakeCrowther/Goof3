[ FunctionDeclaration {
    id: Identifier { name: 'myFunction' },
    parameters:
     [ Parameter { type: 'whole_number', id: Identifier { name: 'p1' } },
       Parameter { type: 'whole_number', id: Identifier { name: 'p2' } } ],
    body:
     [ ReturnStatement {
         returnValue:
          BinaryExpression {
            op: '*',
            left: Identifier { name: 'p1' },
            right: Identifier { name: 'p2' } } } ] },
  CallExpression {
    callee: Identifier { name: 'myFunction' },
    args:
     [ Literal { type: 'number', value: 1 },
       Literal { type: 'number', value: 2 } ] } ]